---
import { db } from '~/db'

const users = await db.query.users.findMany()
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>

		{users.map(user => (
			<div>{user.email}</div>
		))}
	</body>
</html>
